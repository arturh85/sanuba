(
    name: "Food Seeking Test",
    description: "Validates food compass sensor and eating mechanics",

    setup: [
        (type: "FillRect", min_x: -100, min_y: 0, max_x: 100, max_y: 10, material: "stone"),
        (type: "PlaceMaterial", x: 50, y: 30, material: "fruit", radius: 3),
    ],

    actions: [
        (type: "SpawnCreature", genome_type: "worm", x: -50.0, y: 30.0),
        (type: "WaitFrames", frames: 600), // 10 seconds
        (type: "CaptureScreenshot", filename: "food_seeking.png"),
    ],

    verify: [
        (type: "MaterialCountRange",
            material: "fruit",
            region: (type: "Whole"),
            min: 0,
            max: 50), // Some fruit consumed
    ],

    cleanup: [],
)
