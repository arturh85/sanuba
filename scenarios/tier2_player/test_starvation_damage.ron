(
    name: "Starvation Damage Test",
    description: "Validates hunger drains and starvation damage applies",

    setup: [
        (type: "TeleportPlayer", x: 0.0, y: 100.0),
        (type: "SetPlayerHealth", health: 100.0),
        (type: "SetPlayerHunger", hunger: 0.0),
