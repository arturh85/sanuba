(
    name: "Vine Growth Test",
    description: "Tests that VINE grows in random tangled patterns over time.",

    setup: [
        // Place a single active vine tip
        (type: "FillRect", min_x: 32, max_x: 32, min_y: 10, max_y: 10, material: 45), // VINE
        // Note: The vine needs BEHAVIOR_ACTIVE flag to grow, which is set in the material
        // For this test, we'll verify the vine pixel exists
    ],

    actions: [
        // Wait for potential vine growth (probabilistic)
        (type: "WaitFrames", frames: 300), // 5 seconds for vine to grow
    ],

    verify: [
        // The original vine should still exist (may have spread)
        (type: "MaterialAt", x: 32, y: 10, expected: 45), // VINE still present
    ],

    cleanup: [],
)
