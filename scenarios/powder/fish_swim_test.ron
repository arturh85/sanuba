(
    name: "Fish Swimming Test",
    description: "Tests that FISH swims in water and stays within the water body.",

    setup: [
        // Create a water pool (container)
        (type: "FillRect", min_x: 15, max_x: 45, min_y: 10, max_y: 10, material: "stone"),
        (type: "FillRect", min_x: 15, max_x: 15, min_y: 11, max_y: 25, material: "stone"),
        (type: "FillRect", min_x: 45, max_x: 45, min_y: 11, max_y: 25, material: "stone"),
        // Fill with water
        (type: "FillRect", min_x: 16, max_x: 44, min_y: 11, max_y: 24, material: "water"),
        // Place a fish in the water
        (type: "FillRect", min_x: 30, max_x: 30, min_y: 17, max_y: 17, material: "fish"),
    ],

    actions: [
        // Wait for fish to swim
        (type: "WaitFrames", frames: 180), // 3 seconds
    ],

    verify: [
        // Pool walls should still exist
        (type: "MaterialAt", x: 15, y: 15, expected: "stone"),
        (type: "MaterialAt", x: 45, y: 15, expected: "stone"),
        // Fish behavior is probabilistic, so we just verify the scenario runs
    ],

    cleanup: [],
)
