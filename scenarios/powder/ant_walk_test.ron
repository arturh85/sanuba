(
    name: "Ant Random Walk Test",
    description: "Tests that ANT walks on solid ground and avoids falling into air.",

    setup: [
        // Create a solid ground platform
        (type: "FillRect", min_x: 10, max_x: 50, min_y: 10, max_y: 10, material: 1), // STONE ground
        // Place an ant on the platform
        (type: "FillRect", min_x: 30, max_x: 30, min_y: 11, max_y: 11, material: 48), // ANT
    ],

    actions: [
        // Wait for ant to walk around
        (type: "WaitFrames", frames: 120), // 2 seconds
    ],

    verify: [
        // Ground should still exist
        (type: "MaterialAt", x: 30, y: 10, expected: 1), // STONE
        // Ant behavior is probabilistic, so we just verify the scenario runs
    ],

    cleanup: [],
)
